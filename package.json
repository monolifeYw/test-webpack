    "build:dev:analysis": "NODE_ENV=development webpack --profile --env.analysis",
    "debug:wp": "DEBUG=webpack:* nodemon server.js",
    "debug": "nodemon --inspect --debug-brk server.js",
    "inspector": "node-inspector --web-port=8888",
    "start": "nodemon server.js"
  },
